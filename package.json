{
  "name": "Cornet.JS",
  "version": "0.1.1",
  "description": "The Humane JavaScript Library for Modern and Honest Web Notifications. An open source project by Toby56 under the GNU General Public License.",
  "main": "/cornet.js",
  "author": "Toby56",
  "website": "https://cornet-js.github.io/",
  "license": "GNU General Public License",
  "devDependencies": {
    "xo": "0.18.2",
    "eslint": "4.2.0"
  },
  "eslintConfig": {
    "rules": {
      "quotes": [1, "double"],
      "quote-props": [1, "consistent-as-needed"],
      "no-var": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "no-prototype-builtins": 0
    }
  },
  "xo": {
    "rules": {
      "quotes": [1, "double"],
      "quote-props": [1, "consistent-as-needed"],
      "no-var": 0,
      "object-shorthand": 0,
      "prefer-arrow-callback": 0,
      "no-prototype-builtins": 0
    },
    "semicolon": true,
    "space": 2,
    "envs": ["browser"],
    "ignores": ["**.min.js", "**.html", "**/lib/**.js"]
  }
}
