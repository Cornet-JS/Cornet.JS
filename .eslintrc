{
  "rules": {
    "quotes": [1, "double"],
    "quote-props": [1, "consistent-as-needed"],
    "max-depth": [1, 8],
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "no-prototype-builtins": 0
  },
  "semicolon": true,
  "space": 2,
  "envs": ["browser"],
  "ignores": ["**.min.js", "**.html", "**/lib/**.js"]
}
